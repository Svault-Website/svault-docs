{
  "name": "@mintlify/cli",
  "version": "3.0.69",
  "description": "The Mintlify CLI",
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Mintlify, Inc.",
  "bugs": {
    "url": "https://github.com/mintlify/docs/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "cli"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": "./bin/index.js",
  "bin": {
    "mintlify": "bin/index.js"
  },
  "scripts": {
    "dev": "yarn build && node bin/index.js dev",
    "installDeps": "yarn build && node bin/index.js install",
    "prepare": "npm run build",
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint src --cache",
    "test": "jest",
    "format": "prettier \"./src/**/*.ts\" --write",
    "format:check": "prettier \"./src/**/*.ts\" --check"
  },
  "jest": {
    "preset": "ts-jest",
    "resolver": "ts-jest-resolver",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ]
  },
  "dependencies": {
    "@mintlify/link-rot": "3.0.14",
    "@mintlify/previewing": "3.1.66",
    "@mintlify/validation": "0.1.52",
    "chalk": "^5.2.0",
    "detect-port": "^1.5.1",
    "inquirer": "^9.2.7",
    "shelljs": "^0.8.5",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@mintlify/eslint-config": "1.0.3",
    "@mintlify/eslint-config-typescript": "1.0.7",
    "@mintlify/prettier-config": "1.0.1",
    "@mintlify/ts-config": "1.0.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@trivago/prettier-plugin-sort-imports": "3.x",
    "@tsconfig/recommended": "1.x",
    "@types/detect-port": "^1.3.2",
    "@types/inquirer": "^9.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.7.13",
    "@types/shelljs": "^0.8.11",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "5.x",
    "@typescript-eslint/parser": "5.x",
    "eslint": "8.x",
    "eslint-config-prettier": "8.x",
    "eslint-plugin-unused-imports": "2.x",
    "jest": "^29.4.3",
    "prettier": "2.x",
    "ts-jest": "^29.0.5",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^4.9.5"
  },
  "gitHead": "e4459866dcb12fa74b22d5a5775dde1a0f14688a"
}
